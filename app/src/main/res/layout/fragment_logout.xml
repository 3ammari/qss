<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
    android:layout_height="match_parent" tools:context="customer.quick.source.qss.LogoutFragment">


    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_margin="5dp"
        android:background="#ffe9e0d6">

        <TextView
       android:layout_width="wrap_content"
       android:layout_height="wrap_content"
       android:text="@string/logOutConfirmMsg"
       android:layout_centerHorizontal="true"
       android:textSize="18sp"
       android:textColor="#0393A6"
       android:id="@+id/confirmTV"
       android:layout_margin="5dp"
       />

        <Button
       android:layout_width="100dp"
       android:layout_height="wrap_content"
       android:text="@string/logOutConfirm"
       android:textSize="14sp"
       android:background="#0393A6"
       android:id="@+id/yesButton"
            android:layout_below="@+id/confirmTV"
            android:layout_margin="5dp"
            android:layout_centerHorizontal="true"
       />

</RelativeLayout>
</FrameLayout>
